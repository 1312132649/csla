<UserControl x:Class="NavigationApp.Page"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="400" Height="300">
    <Grid x:Name="LayoutRoot" Background="LightBlue">
    <StackPanel Orientation="Vertical">
      <StackPanel Orientation="Horizontal" Height="30">
        <Button x:Name="LaunchControlOne" Content="Control One" Click="ControlOne_Click"/>
        <Button x:Name="LaunchControlTwo" Content="Control Two" Click="ControlTwo_Click"/>
      </StackPanel>
      <ContentControl x:Name="PlaceHolder">
      </ContentControl>
    </StackPanel>
   
   
  </Grid>
</UserControl>
