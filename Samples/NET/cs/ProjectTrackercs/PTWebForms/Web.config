<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appSettings>
    <add key="CslaAuthentication" value="Csla" />
    <add key="CslaDataPortalProxy" value="Csla.DataPortalClient.WcfProxy, Csla" />
  </appSettings>
  <connectionStrings />
  <system.serviceModel>
    <client>
      <endpoint name="WcfDataPortal" address="http://localhost:22627/WcfPortal.svc" binding="wsHttpBinding" contract="Csla.Server.Hosts.IWcfPortal" />
    </client>
  </system.serviceModel>
  <system.web>
    <pages theme="Basic">
      <controls>
        <add tagPrefix="csla" assembly="Csla.Web" namespace="Csla.Web"/>
      </controls>
    </pages>
    <compilation debug="true" defaultLanguage="c#" />
    <authorization>
      <allow users="*" />
    </authorization>
    <authentication mode="Forms" />
    <membership defaultProvider="PTMembershipProvider">
      <providers>
        <add name="PTMembershipProvider" type="PTWebForms.PTMembershipProvider" enablePasswordRetrieval="false" enablePasswordReset="false" requiresQuestionAndAnswer="false" applicationName="/" requiresUniqueEmail="false" passwordFormat="Clear" description="Stores and retrieves membership &#xA;            data using CSLA .NET business objects."/>
      </providers>
    </membership>
  </system.web>
  <system.webServer>
    <directoryBrowse enabled="false" />
    <defaultDocument>
      <files>
        <clear />
        <add value="default.aspx" />
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>
